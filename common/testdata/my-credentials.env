# Service1 configured with Auth Session
SERVICE1_AUTH_TYPE=COUCHDB_SESSION
SERVICE1_URL=https://my-couch-1
SERVICE1_USERNAME=my-username
SERVICE1_PASSWORD=my-password

# Service2 configured with Auth Session in lower case
SERVICE2_AUTH_TYPE=couchdb_session
SERVICE2_URL=https://my-couch-2
SERVICE2_DISABLE_SSL=true
SERVICE2_USERNAME=my-username
SERVICE2_PASSWORD=my-password

# Service3 configured with IAM
SERVICE3_AUTH_TYPE=IAM
SERVICE3_URL=https://my-couch-3
SERVICE3_APIKEY=my-api-key

# Service4 configured with IAM and auth type alias
SERVICE4_AUTHTYPE=IAM
SERVICE4_URL=https://my-couch-4
SERVICE4_APIKEY=my-api-key
